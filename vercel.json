{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ],
    "commands": {
        "build": "pip install -r requirements.txt",
        "start": "gunicorn -w 4 -b 0.0.0.0:8000 api.app:app"
    }
  }
  